{
  "compilerOptions": {
    "incremental": true,
    "module": "commonjs",
    "lib": ["es6", "es2015", "es2017", "es2019", "es2021", "dom", "webworker"],
    "jsx": "react",
    "moduleResolution": "Node",
    "allowJs": true,
    "outDir": ".erb/dll",
    "allowUnreachableCode": false,
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "sourceMap": true,
    "strictNullChecks": true,
    "strict": false,
    "pretty": true,
    "suppressImplicitAnyIndexErrors": true,
    "useDefineForClassFields": true,
    "target": "esnext",
    "baseUrl": "./src",
    "paths": {
      "@assets/*": ["./renderer/assets/*"],
      "@common/*": ["./renderer/common/*"],
      "@wrappers/*": ["./renderer/common/wrappers/*"],
      "@helpers/*": ["./renderer/common/helpers/*"],
      "@hooks/*": ["./renderer/common/hooks/*"],
      "@context/*": ["./renderer/common/context/*"],
      "@components/*": ["./renderer/common/components/*"],
      "@services/*": ["./renderer/common/services/*"],
      "@constants/*": ["./renderer/constants/*"],
      "@interfaces/*": ["./renderer/interfaces/*"],
      "@pages/*": ["./renderer/pages/*"],
      "@store/*": ["./renderer/store/*"]
    }
  },
  "exclude": ["test", "release/build", "release/app/dist", ".erb/dll"]
}
